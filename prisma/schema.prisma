// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Login {
  id        Int     @id @default(autoincrement())
  username  String  @unique
  password  String
  user_id   Int     @unique
  auth_key  Int
}

model Employee {
  id            Int      @id @default(autoincrement())
  email         String   @unique
  first_name    String
  middle_name   String?
  last_name     String
  phone         Int      @unique
}

model Customer {
  id Int @id @default(autoincrement())
  first_name String
  middle_name String?
  last_name String
  address String
  email String?

}